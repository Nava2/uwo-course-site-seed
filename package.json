{
  "name": "uwo-course-seed",
  "version": "0.0.1",
  "description": "Base template for generating a simple course website",
  "homepage": "https://github.com/Nava2/uwo-course-site-seed",
  "contributors": [
    {
      "name": "Kevin Brightwell",
      "url": "https://github.com/Nava2"
    }
  ],
  "license": "MIT",
  "repository": "github:Nava2/uwo-course-site-seed",
  "bugs": {
    "url": "https://github.com/Nava2/uwo-course-site-seed/issues"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "bootstrap-calendar": "^0.2.4",
    "express": "^4.14.0",
    "font-awesome": "^4.7.0",
    "handlebars": "^4.0.6",
    "highlight.js": "^9.9.0",
    "jquery": "^3.1.1",
    "jstransformer-markdown-it": "^2.0.0",
    "keyboardjs": "^2.3.3",
    "lodash": "^4.17.2",
    "markdown-it": "^8.2.2",
    "markdown-it-container": "^2.0.0",
    "moment": "^2.17.1",
    "pad-left": "^2.1.0",
    "pug": "^2.0.0-beta6",
    "reveal.js": "git+https://github.com/hakimel/reveal.js.git#master",
    "validator": "^6.2.1"
  },
  "devDependencies": {
    "@types/bootstrap": "^3.3.32",
    "@types/express": "^4.0.34",
    "@types/highlight.js": "^9.1.9",
    "@types/jquery": "^2.0.37",
    "@types/keyboardjs": "^2.2.30",
    "@types/keypress": "^2.0.28",
    "@types/lodash": "^4.14.45",
    "@types/node": "^6.0.55",
    "@types/reveal": "^3.3.31",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2016": "^6.16.0",
    "bootstrap-sass": "^3.3.7",
    "del": "^2.2.2",
    "dotenv": "^4.0.0",
    "eslint-config-google": "^0.3.0",
    "expose-loader": "^0.7.1",
    "gh-pages": "^0.12.0",
    "gulp": "gulpjs/gulp#4.0",
    "gulp-cache": "^0.4.5",
    "gulp-clean": "^0.3.2",
    "gulp-data": "^1.2.1",
    "gulp-debug": "^3.0.0",
    "gulp-eslint": "^1.1.1",
    "gulp-exec": "^2.1.3",
    "gulp-filter": "^4.0.0",
    "gulp-identity": "^0.1.0",
    "gulp-less": "^3.3.0",
    "gulp-load-plugins": "^1.4.0",
    "gulp-pug": "^3.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.4",
    "gulp-sass": "^3.0.0",
    "gulp-sourcemaps": "^1.9.1",
    "gulp-typescript": "^3.1.3",
    "gulp-uglifycss": "^1.0.6",
    "gulp-webpack": "^1.5.0",
    "gulp-webserver": "^0.9.1",
    "imports-loader": "^0.7.0",
    "json-loader": "^0.5.4",
    "merge2": "^1.0.3",
    "pump": "^1.0.1",
    "ts-loader": "^1.3.3",
    "typescript": "^2.1.4",
    "webpack": "^1.14.0"
  },
  "engines": {
    "node": ">=0.12"
  },
  "private": true,
  "scripts": {
    "test": "gulp && git status | grep 'working directory clean' >/dev/null || (echo 'Please commit all changes generated by building'; exit 1)"
  },
  "eslintConfig": {
    "extends": "google"
  },
  "teachingSeedConfig": {
    "outDir": "./dist",
    "port": 8000,
    "paths": {
      "assets": {
        "root": "assets/"
      },
      "src": {
        "root": "./src/",
        "lectures": "lectures/",
        "labs": "labs/"
      },
      "templates": {
        "root": "./templates/",
        "lectures": "lectures/",
        "labs": "labs/"
      }
    },
    "decktape": {
      "srcDir": "./decktape-1.0.0",
      "phantomjs": "phantomjs",
      "js": "decktape.js"
    },
    "uglify": {
      "preserveComments": "license",
      "compress": {}
    },
    "reveal": {
      "plugins": [
        "math",
        "notes"
      ],
      "theme": "solarized",
      "path": {
        "plugin": "plugin/",
        "theme": "theme/"
      }
    }
  }
}
